<h2><%= @store_count %> Total Stores</h2>
<table class="table">
  <tr>
    <th>Long Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone Number</th>
    <th>Store Type</th>
  </tr>
  <% @stores.each do |store| %>
  <tr>
      <td><%= link_to store.name, store_path(store.id) %></td>
      <td><%= store.city %></td>
      <td><%= store.distance %></td>
      <td><%= store.phone %></td>
      <td><%= store.type %></td>
  </tr>
  <% end %>
</table>
