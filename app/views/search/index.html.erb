<table>
  <tr>
    <th>Long Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone Number</th>
    <th>Store Type</th>
  </tr>
  <tr>
    <% @stores.each do |store| %>
      <td><%= store.name %></td>
      <td><%= store.city %></td>
      <td><%= store.distance %></td>
      <td><%= store.phone %></td>
      <td><%= store.type %></td>
    <% end %>
  </tr>
</table>
